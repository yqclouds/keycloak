                    <exclude>**/HELP-US-OUT.txt</exclude>
                    <exclude>**/README.md</exclude>
                    <exclude>**/Gruntfile.js</exclude>
                    <exclude>**/Gemfile*</exclude>
                    <exclude>**/.*</exclude>
